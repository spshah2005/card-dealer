#include <stdio.h>

int main() {
    int x;
    char *suit;

    printf("Enter a card number (0-51): ");
    scanf("%d", &x);

    if (x < 0 || x > 51) {  // Ensure valid input
        printf("Invalid card number.\n");
        return -1;
    }

    int card_rank = x % 13;  // Rank of the card

    // Assign suit based on range
    if (x < 13) {
        suit = "Spades";
    } else if (x < 26) {
        suit = "Clubs";
    } else if (x < 39) {
        suit = "Diamonds";
    } else {
        suit = "Hearts";
    }

    // Print the card rank with the correct name
    if (card_rank == 0) {
        printf("The card is: Ace of %s\n", suit);
    } else if (card_rank == 10) {
        printf("The card is: Jack of %s\n", suit);
    } else if (card_rank == 11) {
        printf("The card is: Queen of %s\n", suit);
    } else if (card_rank == 12) {
        printf("The card is: King of %s\n", suit);
    } else {
        printf("The card is: %d of %s\n", card_rank + 1, suit);
    }

    return 0;
}
